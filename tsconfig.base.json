{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2020",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@goup/common-types": ["libs/common/common-types/src/index.ts"],
      "@goup/common-utils": ["libs/common/common-utils/src/index.ts"],
      "@goup/ng-authentication": ["libs/sites/ng-authentication/src/index.ts"],
      "@goup/ng-form-control-error": ["libs/sites/ng-form-control-error/src/index.ts"],
      "@goup/ng-initializer": ["libs/sites/ng-initializer/src/index.ts"],
      "@goup/ng-system-url": ["libs/sites/ng-system-url/src/index.ts"],
      "@goup/ng-translator": ["libs/sites/ng-translator/src/index.ts"],
      "@goup/nx-plugin": ["libs/common/nx-plugin/src/index.ts"],
      "@goup/service-crud": ["libs/services/service-crud/src/index.ts"],
      "@goup/service-grpc": ["libs/services/service-grpc/src/index.ts"],
      "@goup/service-grpc-exceptions": ["libs/services/service-grpc-exceptions/src/index.ts"],
      "@goup/service-grpc-logger": ["libs/services/service-grpc-logger/src/index.ts"],
      "@goup/service-http-logger": ["libs/services/service-http-logger/src/index.ts"],
      "@goup/service-logger": ["libs/services/service-logger/src/index.ts"],
      "@goup/service-protobuf": ["libs/services/service-protobuf/src/index.ts"],
      "@goup/service-testing": ["libs/services/service-testing/src/index.ts"],
      "@goup/service-translator": ["libs/services/service-translator/src/index.ts"],
      "@goup/translator-marker": ["libs/common/translator-marker/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
