{
  "name": "home-site",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "home",
  "sourceRoot": "apps/sites/home-site/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/sites/home-site",
        "index": "apps/sites/home-site/src/index.html",
        "browser": "apps/sites/home-site/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/sites/home-site/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/sites/home-site/public"
          },
          {
            "glob": "i18n-default.json",
            "input": "apps/sites/home-site"
          }
        ],
        "styles": [
          "node_modules/@taiga-ui/core/styles/taiga-ui-theme.less",
          "node_modules/@taiga-ui/core/styles/taiga-ui-fonts.less",
          "apps/sites/home-site/src/styles.scss"
        ],
        "scripts": [],
        "server": "apps/sites/home-site/src/main.server.ts",
        "prerender": true,
        "ssr": {
          "entry": "apps/sites/home-site/server.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "preview": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/sites/home-site/src/environments/environment.ts",
              "with": "apps/sites/home-site/src/environments/environment.preview.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "apps/sites/home-site/src/environments/environment.ts",
              "with": "apps/sites/home-site/src/environments/environment.development.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "options": {
        "host": "lacolhost.com",
        "port": 4200
      },
      "configurations": {
        "production": {
          "buildTarget": "home-site:build:production"
        },
        "preview": {
          "buildTarget": "home-site:build:preview"
        },
        "development": {
          "buildTarget": "home-site:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "i18n-extract": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/sites/home-site",
        "command": "npx ngx-translate-extract -i ./src -o ./i18n-default.json -m marker -d -c"
      }
    },
    "i18n-lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/sites/home-site",
        "command": "npx ngx-translate-lint -c i18n-lint.json"
      }
    },
    "lint": {
      "dependsOn": ["i18n-lint"],
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/sites/home-site/jest.config.ts"
      }
    }
  }
}
