{
  "$schema": "https://json-schema.org/schema",
  "version": 2,
  "title": "ProtoToTs executor",
  "description": "Schema for ProtoToTs executor",
  "type": "object",
  "properties": {
    "input": {
      "type": "string",
      "description": "The path to the proto files"
    },
    "output": {
      "type": "string",
      "description": "The output directory for generated TypeScript files"
    }
  },
  "required": ["input", "output"]
}
